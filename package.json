{
  "name": "social-game",
  "version": "0.2.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "prebuild": "expo prebuild",
    "lint": "eslint \"**/*.{js,jsx,ts,tsx}\" && prettier -c \"**/*.{js,jsx,ts,tsx,json}\"",
    "lint:fix": "eslint \"**/*.{js,jsx,ts,tsx}\" --fix",
    "format": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --write",
    "format:check": "prettier \"**/*.{js,jsx,ts,tsx,json}\" --check",
    "type-check": "tsc --noEmit",
    "validate": "pnpm run lint && pnpm run type-check",
    "clean": "rm -rf node_modules .expo android ios",
    "reset": "pnpm run clean && pnpm install && npx expo start --clear",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "expo": "^54.0.0",
    "expo-font": "~14.0.11",
    "expo-haptics": "~15.0.8",
    "expo-status-bar": "~3.0.8",
    "nativewind": "^4.2.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-web": "^0.21.2",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@commitlint/cli": "^20.3.1",
    "@commitlint/config-conventional": "^20.3.1",
    "@types/react": "~19.1.10",
    "eslint": "^9.25.1",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.2",
    "husky": "^9.1.7",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.4.0",
    "typescript": "~5.9.2"
  },
  "main": "node_modules/expo/AppEntry.js",
  "private": true
}
